<!DOCTYPE hibernate-configuration PUBLIC 	"-//Hibernate/Hibernate Configuration DTD//EN"	"http://hibernate.sourceforge.net/hibernate-configuration-2.0.dtd">
<hibernate-configuration>	<session-factory>		<property name="hibernate.connection.url">			jdbc:mysql://localhost/leavearmycountdown</property>		<property name="hibernate.connection.driver_class">			org.gjt.mm.mysql.Driver</property>		<property name="hibernate.connection.username">root</property>		<property name="hibernate.connection.password">password</property>		<property name="dialect">net.sf.hibernate.dialect.MySQLDialect</property>		<property name="hibernate.use_outer_join">True</property>		<property name="show_sql">false</property>		<property name="hibernate.transaction.factory_class">			net.sf.hibernate.transaction.JDBCTransactionFactory</property>				<!-- c3p0 config -->			<property name="hibernate.c3p0.acquire_increment">5</property>        <property name="hibernate.c3p0.min_size">10</property>        <property name="hibernate.c3p0.max_size">100</property>        <property name="hibernate.c3p0.timeout">600</property>        <property name="hibernate.c3p0.max_statements">80</property>        <property name="hibernate.c3p0.idle_test_period">120</property>        <property name="hibernate.c3p0.validate">true</property>				<!-- Mapping Resources -->		<mapping resource="com/franklin/logoutarmycd/core/storage/db/po/Logoutarmycduser0.hbm.xml"/>		<mapping resource="com/franklin/logoutarmycd/core/storage/db/po/Logoutarmycduser1.hbm.xml"/>		<mapping resource="com/franklin/logoutarmycd/core/storage/db/po/Logoutarmycduser2.hbm.xml"/>		<mapping resource="com/franklin/logoutarmycd/core/storage/db/po/Logoutarmycduser3.hbm.xml"/>		<mapping resource="com/franklin/logoutarmycd/core/storage/db/po/Logoutarmycduser4.hbm.xml"/>		<mapping resource="com/franklin/logoutarmycd/core/storage/db/po/Logoutarmycduser5.hbm.xml"/>		<mapping resource="com/franklin/logoutarmycd/core/storage/db/po/Logoutarmycduser6.hbm.xml"/>		<mapping resource="com/franklin/logoutarmycd/core/storage/db/po/Logoutarmycduser7.hbm.xml"/>		<mapping resource="com/franklin/logoutarmycd/core/storage/db/po/Logoutarmycduser8.hbm.xml"/>		<mapping resource="com/franklin/logoutarmycd/core/storage/db/po/Logoutarmycduser9.hbm.xml"/>	</session-factory></hibernate-configuration>